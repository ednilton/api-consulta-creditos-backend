spring:
  datasource:
    url: jdbc:postgresql://${POSTGRES_HOST_TEST}:${POSTGRES_PORT_TEST}/${POSTGRES_DB_TEST}
    username: ${POSTGRES_USER_TEST}
    password: ${POSTGRES_PASSWORD_TEST}
  jpa:
    hibernate:
      ddl-auto: validate
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect
